type hal_fingerprint_g, domain, binder_in_vendor_violators;
hal_server_domain(hal_fingerprint_g, hal_fingerprint)

type hal_fingerprint_g_exec, exec_type, vendor_file_type, file_type;
binder_use(hal_fingerprint_g)
init_daemon_domain(hal_fingerprint_g)

allow hal_fingerprint_g gx_fpd:binder { transfer call };
allow hal_fingerprint_g gx_fpd_service:service_manager find;
allow hal_fingerprint_g fingerprint_service:service_manager find;
allow hal_fingerprint_g fpc_sysfs:file rw_file_perms;
allow hal_fingerprint_g fpc_sysfs:dir rw_dir_perms;
allow hal_fingerprint_g tee_device:chr_file rw_file_perms;
allow hal_fingerprint_g uhid_device:chr_file rw_file_perms;
allow hal_fingerprint_g fpc_data_file:dir rw_dir_perms;
allow hal_fingerprint_g fpc_data_file:sock_file create_file_perms;
allow hal_fingerprint_g servicemanager:binder { transfer call };

allow hal_fingerprint_g fingerprintd_data_file:dir create_dir_perms;
allow hal_fingerprint_g fingerprintd_data_file:file create_file_perms;
allow hal_fingerprint_g media_rw_data_file:dir { open read search };
allow hal_fingerprint_g mnt_user_file:dir search;
allow hal_fingerprint_g mnt_user_file:lnk_file read;
allow hal_fingerprint_g sdcardfs:dir search;
allow hal_fingerprint_g storage_file:dir search;
allow hal_fingerprint_g storage_file:lnk_file read;

set_prop(hal_fingerprint_g, system_prop)
r_dir_file(hal_fingerprint_g, firmware_file)

add_service(hal_fingerprint_g, hal_fingerprint_g_service);

# Need to add auth tokens to KeyStore
use_keystore(hal_fingerprint_g)
allow hal_fingerprint_g keystore:keystore_key { add_auth };
# need to find KeyStore and add self
allow hal_fingerprint_g hal_fingerprint_g_service:service_manager { add find };

allow hal_fingerprint_g keystore_service:service_manager find;

# For permissions checking
binder_call(hal_fingerprint_g, system_server);
allow hal_fingerprint_g permission_service:service_manager find;
